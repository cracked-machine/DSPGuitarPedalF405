#include <ResourceManager.hpp>
#include <new>

void* operator new(size_t size)
{
    cout<< "Overloading new operator with size: " << size << endl;
	//void * p = ::operator new(size);
	void * p = malloc(size); //will also work fine
	usedmem++;
	return p;
}


