#ifndef DSP_IIRFILTERFX_HPP_
#define DSP_IIRFILTERFX_HPP_


#include "AbstractFx.hpp"

class IIRFilterFx : public AbstractFx
{
public:
	void process(	array<uint16_t, STEREO_BLOCK_SIZE_U16> *pRxBuf,
					array<uint16_t, STEREO_BLOCK_SIZE_U16> *pTxBuf) override;

	void setBufferState(AbstractFx::BufferState pBufferState) override;
	AbstractFx::BufferState getBufferState() override;
};




#endif /* DSP_IIRFILTERFX_HPP_ */
