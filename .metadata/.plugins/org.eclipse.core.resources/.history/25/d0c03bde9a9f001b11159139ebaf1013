#ifndef FSM_FXDISABLEDSTATE_HPP_
#define FSM_FXDISABLEDSTATE_HPP_

#include "AbstractState.hpp"
#include <ExtCtrlStateMachine.hpp>
class ExtCtrlStateMachine;


class FxDisabledState : public AbstractState
{
public:


	 void evFootswitchA(ExtCtrlStateMachine *machine);
	virtual void evFootswitchB(ExtCtrlStateMachine *machine);
	virtual void evUserSwitchA(ExtCtrlStateMachine *machine);
	virtual void evUserSwitchB(ExtCtrlStateMachine *machine);
	virtual void evRotaryEncoderA(ExtCtrlStateMachine *machine);
	virtual void evRotaryEncoderB(ExtCtrlStateMachine *machine);

};


#endif /* FSM_FXDISABLEDSTATE_HPP_ */
