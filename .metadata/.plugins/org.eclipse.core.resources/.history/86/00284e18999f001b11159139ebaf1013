//#include "BaseState.hpp"

//class FxEnabledState : public BaseState
class FxEnabledState : public AbstractState
{
public:


	virtual void evFootswitchA(ExtCtrlStateMachine *machine);
	virtual void evFootswitchB(ExtCtrlStateMachine *machine);
