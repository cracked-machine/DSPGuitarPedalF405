#include <DSPManager.hpp>

DSPManager::DSPManager(AbstractFx* pFx)
{
	if(pFx != nullptr)
		theFx = pFx;
}

DSPManager::~DSPManager()
{
	delete theFx;
}

void
DSPManager::setFx(AbstractFx* pFx)
{
	if(pFx != nullptr)
		theFx = pFx;
}

AbstractFx*
DSPManager::getFx()
{
	return theFx;
}

void
DSPManager::setRx(StereoBlockU16< AbstractFx::FULL_BLK_SIZE_U16 > *pBuf)
{
	&this->rxBuf = pBuf;
}

void
DSPManager::setTx(StereoBlockU16< AbstractFx::FULL_BLK_SIZE_U16 > *pBuf)
{
	txBuf = pBuf;
}

StereoBlockU16< AbstractFx::FULL_BLK_SIZE_U16 > *
DSPManager::getRx()
{
	return rxBuf;
}

StereoBlockU16< AbstractFx::FULL_BLK_SIZE_U16 > *
DSPManager::getTx()
{
	return txBuf;
}
