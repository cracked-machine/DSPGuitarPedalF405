#ifndef DEBOUNCE_DEBOUNCE_HPP_
#define DEBOUNCE_DEBOUNCE_HPP_


template <class T>
class DebounceManager
{

public:
	DebounceManager(T _tim)
	{
		tim = _tim;
	}

private:
	T tim;

};

typedef DebounceManager<TIM_HandleTypeDef> HALDebounceManager;


#endif /* DEBOUNCE_DEBOUNCE_HPP_ */
