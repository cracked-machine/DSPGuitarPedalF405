#include <AbstractState.hpp>

void AbstractState::setState(EventMachine &machine, AbstractState *state) {
  machine.state = state;
}


void FxDisabledState::update()
{

}


void FxEnabledState::update()
{
	switch(machine.state)
	{
	case FxDisabledState:
		break;
	case FxEnabledState:
		break;
	}
}
