#include "DSPManager.hpp"

DSPManager::DSPManager()
{
	theFx = new IIRFilterFx();
}

DSPManager::~DSPManager()
{
	delete theFx;
}

void DSPManager::setFx(AbstractFx* pFx)
{
	// do nothing?
}

AbstractFx* DSPManager::getFx()
{
	return theFx;
}
