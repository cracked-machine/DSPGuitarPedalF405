#include "EventMachine.hpp"

EventMachine::EventMachine()
{
	state = stateFX_DISABLED;
}

void EventMachine::update(EventMachine &machine)
{
	state->update(EventMachine &machine);
}

AbstractState * EventMachine::getState()
{
	return state;
}
