#include <AbstractStateContainer.hpp>



ExtCtrlStateContainer::ExtCtrlStateContainer(std::array<AbstractState*, MAX_EXTCTRL_STATES> *pStates)
{
	states = pStates;
}

size_t ExtCtrlStateContainer::getSize()
{
	return _size;
}
